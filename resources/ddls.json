          "table_name": "tracking_orders",
          "description": "Contains data regarding the journey of the order and tracking information from hub to hub",
          "columns": [
            {
              "column_name": "track_id",
              "data_type": "uuid",
              "description": "Primary key, unique identifier for tracking record"
            },
            {
              "column_name": "order_id",
              "data_type": "uuid",
              "description": "Order identifier, links to orders table"
            },
            {
              "column_name": "original_order_id",
              "data_type": "VARCHAR(63)",
              "description": "Seller's order ID"
            },
            {
              "column_name": "carrier_name",
              "data_type": "VARCHAR(63)",
              "description": "Name of the carrier (courier partner)"
            },
            {
              "column_name": "current_order_status",
              "data_type": "VARCHAR(31)",
              "description": "Current tracking status of the shipment. Possible values: CANCELLED, ORDERED, PICKED UP, IN TRANSIT, OUT FOR DELIVERY, DELIVERED, UNDELIVERED, RTO IN TRANSIT, RTO DELIVERED, RTO, RTO OFD, DELAYED, LOST, DAMAGED, OUT FOR PICKUP, PICKUP RESCHEDULED, PICKUP EXCEPTION, RTO INITIATED, RTO NDR, MISROUTED, CHANNEL_ORDER"
            },
            {
              "column_name": "current_order_status_code",
